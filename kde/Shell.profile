[Appearance]
AntiAliasFonts=true
ColorScheme=ompugao
Font=Source Code Pro Black for Powerline,11.5,-1,5,87,0,0,0,0,0

[General]
Environment=TERM=xterm
Icon=utilities-terminal
LocalTabTitleFormat=%D : %n
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=(%u) %H
SaveGeometryOnExit=true
ShowMenuBar=false
ShowNewAndCloseTabButtons=false
ShowTerminalSizeHint=true
StartInCurrentSessionDir=true
TabBarMode=1

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=true
BlinkingCursorEnabled=true
